{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-hide",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "clipboard-manager:allow-write",
    "event:allow-emit",
    "shell:allow-open"
  ]
}